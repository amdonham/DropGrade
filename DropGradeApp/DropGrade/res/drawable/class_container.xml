<?xml version="1.0" encoding="utf-8"?>
<!--  Layout for the container of a class. -->
<shape
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:shape="rectangle">
  <solid android:color="#44C9FFFF" />
  <stroke android:color="#0000"
      android:width="4px" />
  <padding
    android:left="5dp"
    android:right="5dp"
    android:top="5dp"
    android:bottom="5dp" />
  <corners
      android:radius="5dp"/>
  
</shape>  